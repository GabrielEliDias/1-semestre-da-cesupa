#include <stdio.h>

int main() {
  int v[15], i;
  for (i = 0; i<15; i++){
    printf("v[%d]:", i);
    scanf("%d", &v[i]);
  }
  int j;
  for (j=0;j<15;j++){
    if (v[j]% 2== 0){
      printf("vetor[%d] eh par", v[j]);
    }
    else {
      printf("vetor[%d] eh impar", v[j]);
    }
     printf("\n");
  }
  return 0;
}
